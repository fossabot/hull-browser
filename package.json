{
  "name": "hull-client",
  "description": "Access customer properties from the client side",
  "version": "0.0.2",
  "homepage": "https://github.com/hull-ships/hull-client",
  "license": "MIT",
  "main": "npm run start",
  "ui": true,
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-client"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-client/issues"
  },
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain client",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:server": "babel ./server -d ./build",
    "build:client": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.js --progress --profile --colors",
    "start": "node ./build",
    "start:dev": "NODE_ENV=development babel-watch -L ./server",
    "test": "npm run test:lint && npm run test:modules",
    "test:lint": "./node_modules/.bin/eslint server",
    "test:modules": "npm outdated --depth=0",
    "clean": "./node_modules/.bin/rimraf dist; ./node_modules/.bin/rimraf build",
    "update": "./node_modules/.bin/updtr",
    "watch": "./node_modules/.bin/watch 'npm run test:lint && npm run build:client' src server",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship"
  ],
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "supertest": "^3.0.0",
    "updtr": "^0.2.3",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-watch": "^2.0.6",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "connect": "^3.5.1",
    "cors": "^2.8.1",
    "ejs": "^2.5.6",
    "event-stream": "^3.3.4",
    "eventemitter": "^0.3.3",
    "eventemitter2": "^3.0.1",
    "express": "^4.14.1",
    "hull": "0.10.7",
    "jose": "^0.3.2",
    "jquery": "^3.1.1",
    "jwt-simple": "^0.5.1",
    "librato-node": "^4.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "newrelic": "^1.37.2",
    "object-mapper": "^3.0.1",
    "query-string": "^4.3.2",
    "simple-encryptor": "^1.1.0",
    "uri": "^0.1.0",
    "urijs": "^1.18.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "winston-logstash": "^0.3.0"
  }
}
